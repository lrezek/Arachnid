{
    "name": "lrezek/arachnid",
    "type": "library",
    "description": "PHP Object Graph Mapper for Neo4J.",
    "keywords": ["neo4j","graph","database","entity-mapper","doctrine"],
    "homepage": "https://github.com/lrezek/Arachnid",
    "license": "MIT",
    "authors": [
        {"name": "Lukas Rezek", "email": "lukas@miratronix.com", "homepage": "http://www.miratronix.com"}
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": ">=2.2.0,<2.4.0-dev",
        "everyman/neo4jphp": ">=0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.19",
        "satooshi/php-coveralls": "dev-master",
        "phpdocumentor/phpdocumentor": "2.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "archive": {
        "exclude": ["*/Tests", "phpunit.xml", "phpdoc.xml", ".travis.yml", ".coveralls.yml"]
    }
}
